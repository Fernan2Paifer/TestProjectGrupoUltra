<script>


    function validaNumericos() {
        var inputtxt = document.getElementById('text');
        var valor = inputtxt.value;
        for (i = 0; i < valor.length; i++) {
            var code = valor.charCodeAt(i);
            if (code <= 48 || code >= 57) {
                inputtxt.value = "";
                return;
            }
        }

    }


    function validarInput() {
        if (document.getElementById("text").value == "" || document.getElementById("text").value <= 0) {
            document.getElementById("btn_Validar").disabled = true;
        } else {
            document.getElementById("btn_Validar").disabled = false;
        }
    }

    function validarInputAhorro() {
        if (document.getElementById("textAhorro").value == "" || document.getElementById("textAhorro").value <= 0) {
            document.getElementById("btn_ValidarAhorro").disabled = true;
        } else {
            document.getElementById("btn_ValidarAhorro").disabled = false;
        }
    }

</script>